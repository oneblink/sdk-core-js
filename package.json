{
  "name": "@oneblink/sdk-core",
  "description": "OneBlink SDK for JavaScript (works in Browsers and NodeJS)",
  "version": "9.1.0-beta.3",
  "author": "OneBlink <developers@oneblink.io> (https://oneblink.io)",
  "bugs": {
    "url": "https://github.com/oneblink/sdk-core-js/issues"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3.3.3",
    "@eslint/js": "^9.39.2",
    "@microsoft/eslint-plugin-sdl": "^1.1.0",
    "@microsoft/microsoft-graph-types": "^2.43.1",
    "@oneblink/release-cli": "^4.0.0-beta.2",
    "@oneblink/types": "github:oneblink/types",
    "@salesforce/types": "^1.6.0",
    "@spotto/contract": "^1.0.69-alpha.13",
    "@types/lodash": "^4.17.23",
    "@typescript-eslint/eslint-plugin": "^8.53.0",
    "@typescript-eslint/parser": "^8.53.0",
    "eslint": "^9.39.2",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-prettier": "^5.5.4",
    "fixpack": "^4.0.0",
    "globals": "^17.0.0",
    "lodash": "^4.17.23",
    "prettier": "^3.7.4",
    "prettier-plugin-jsdoc": "^1.8.0",
    "tsc-watch": "^7.2.0",
    "typedoc": "^0.28.16",
    "typescript": "^5.9.3",
    "vitest": "^4.0.17"
  },
  "directories": {
    "doc": "docs"
  },
  "engines": {
    "node": ">=24",
    "npm": ">=11"
  },
  "files": [
    "dist"
  ],
  "homepage": "https://github.com/oneblink/sdk-core-js",
  "keywords": [],
  "license": "GPL-3.0-only",
  "main": "dist/index.js",
  "module": "dist/index.js",
  "publishConfig": {
    "access": "public"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/oneblink/sdk-core-js.git"
  },
  "scripts": {
    "build": "tsc",
    "docs": "typedoc",
    "eslint": "eslint --fix --cache .",
    "fixpack": "fixpack",
    "format": "prettier --write .",
    "prepare": "npm run build",
    "pretest": "npm run fixpack && npm run eslint",
    "release": "oneblink-release repository --no-name",
    "start": "tsc-watch",
    "test": "vitest run",
    "types": "npm install --save-dev github:oneblink/types",
    "typescript": "tsc --noEmit",
    "update-dependents": "oneblink-release update-dependents --force"
  },
  "type": "module"
}
