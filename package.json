{
  "name": "@oneblink/sdk-core",
  "description": "OneBlink SDK for JavaScript (works in Browsers and NodeJS)",
  "version": "8.1.0",
  "author": "OneBlink <developers@oneblink.io> (https://oneblink.io)",
  "bugs": {
    "url": "https://github.com/oneblink/sdk-core-js/issues"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3.3.0",
    "@eslint/js": "^9.21.0",
    "@microsoft/eslint-plugin-sdl": "^1.1.0",
    "@microsoft/microsoft-graph-types": "^2.40.0",
    "@oneblink/release-cli": "^3.3.1-beta.3",
    "@oneblink/types": "github:oneblink/types",
    "@types/jest": "^29.5.14",
    "@types/lodash": "^4.17.16",
    "@typescript-eslint/eslint-plugin": "^8.28.0",
    "@typescript-eslint/parser": "^8.28.0",
    "eslint": "^9.23.0",
    "eslint-config-prettier": "^10.1.1",
    "eslint-plugin-prettier": "^5.2.5",
    "eslint-plugin-react": "^7.37.4",
    "fixpack": "^4.0.0",
    "globals": "^16.0.0",
    "jest": "^29.7.0",
    "lodash": "^4.17.21",
    "prettier": "^3.5.3",
    "prettier-plugin-jsdoc": "^1.3.2",
    "ts-jest": "^29.3.0",
    "tsc-watch": "^6.2.1",
    "typedoc": "^0.28.1",
    "typescript": "^5.8.2"
  },
  "directories": {
    "doc": "docs"
  },
  "engines": {
    "node": ">=18",
    "npm": ">=10"
  },
  "files": [
    "dist"
  ],
  "homepage": "https://github.com/oneblink/sdk-core-js",
  "keywords": [],
  "license": "GPL-3.0-only",
  "main": "dist/index.js",
  "module": "dist/index.js",
  "publishConfig": {
    "access": "public"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/oneblink/sdk-core-js.git"
  },
  "scripts": {
    "build": "tsc",
    "docs": "typedoc",
    "eslint": "eslint --fix --cache .",
    "fixpack": "fixpack",
    "format": "prettier --write .",
    "prepare": "npm run build",
    "pretest": "npm run fixpack && npm run eslint",
    "release": "oneblink-release repository --no-name",
    "start": "tsc-watch",
    "test": "jest",
    "types": "npm install --save-dev github:oneblink/types",
    "typescript": "tsc --noEmit",
    "update-dependents": "oneblink-release update-dependents"
  }
}
